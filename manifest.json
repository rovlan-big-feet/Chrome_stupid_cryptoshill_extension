{
  "name": "Cryptoshill extension",
  "version": "1.0",
  "description": "Counts dumb shitcoins properties or something idk",
  "permissions": ["tabs", "pageCapture", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/coin16.png",
      "32": "images/coin32.png",
      "48": "images/coin48.png",
      "128": "images/coin128.png"
    }
  },
  "icons": {
    "16": "images/coin16.png",
    "32": "images/coin32.png",
    "48": "images/coin48.png",
    "128": "images/coin128.png"
  },
  "manifest_version": 2
}
